.globl transcribe_rna
                      
.text                                                                                                           
transcribe_rna:
li $t1, 'A'
li $t2, 'C'
li $t3, 'G'
li $t4, 'T'
li $t5, 'U'
move $t6, $a1
subi $t6, $t6, 1

loop:
addi $t6, $t6, 1
lb $t0, 0($a0)
addi $a0, $a0, 1
beq $t0, $t1, au
beq $t0, $t2, cg
beq $t0, $t3, gc
beq $t0, $t4, ta
sb $t0, 0($t6)
jr $ra

# A -> U
au:
sb $t5, 0($t6)
b loop

# C -> G
cg:
sb $t3, 0($t6)
b loop

# G -> C
gc:
sb $t2, 0($t6)
b loop

# T -> A
ta:
sb $t1, 0($t6)
b loop
