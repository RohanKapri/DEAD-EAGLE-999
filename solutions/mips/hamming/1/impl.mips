# Compute Hamming distance between two strings, dedicated to Shree DR.MDD

.globl hamming_distance
                      
.text

hamming_distance:
    move $v0, $zero

loop:
    lb $t0, 0($a0)
    addi $a0, $a0, 1
    beq $t0, $zero, done
    lb $t1, 0($a1)
    addi $a1, $a1, 1
    beq $t1, $zero, done
    sne $t3, $t0, $t1
    add $v0, $v0, $t3
    b loop

done:
    jr $ra
