.globl score

score:
        mul     $t0, $a0, $a0
        mul     $t1, $a1, $a1
        add     $t2, $t0, $t1

        li      $v0, 10
        ble     $t2, 1, return

        li      $v0, 5
        ble     $t2, 25, return

        li      $v0, 1
        ble     $t2, 100, return

        move    $v0, $zero

return:
        jr      $ra
