!yamlscript/v0

orbital-periods =::
  Mercury: 0.2408467
  Venus: 0.61519726
  Earth: 1.0
  Mars: 1.8808158
  Jupiter: 11.862615
  Saturn: 29.447498
  Uranus: 84.016846
  Neptune: 164.79132

seconds-per-earth-year =: 31557600

defn age(planet seconds):
  when-not orbital-periods.$planet:
    die: 'not a planet'
    
  earth-years =: seconds / seconds-per-earth-year
  result =: earth-years / orbital-periods.$planet
  =>: round(result * 100) / 100