!yamlscript/v0

defn translate(a):
  case a:
    ('AUG'): 'Methionine'
    ('UUU' 'UUC'): 'Phenylalanine'
    ('UUA' 'UUG'): 'Leucine'
    ('UCU' 'UCC' 'UCA' 'UCG'): 'Serine'
    ('UAU' 'UAC'): 'Tyrosine'
    ('UGU' 'UGC'): 'Cysteine'
    ('UGG'): 'Tryptophan'
    ('UAA' 'UAG' 'UGA'): 'STOP'
    else: 'Invalid'

defn proteins(strand):
  res =: strand.re-seq(/.{1,3}/).map(translate).take-while(\(_ != 'STOP'))
  if res.has?('Invalid'):
    die('Invalid codon')
    res